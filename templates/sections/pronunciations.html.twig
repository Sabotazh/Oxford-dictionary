<div class="row mt-5">
    <div class="col-md-12">
        <h3>Pronunciations</h3>
        <figure class="mt-3">
            {% for entry in result %}
                {% for pronansiation in entry.pronansiations %}
                    {% include 'elements/audio.html.twig' with {'url': pronansiation, 'type': 'audio/mpeg'} %}
                    <div class="w-100"></div>
                {% endfor %}
            {% endfor %}
        </figure>
    </div>
</div>
